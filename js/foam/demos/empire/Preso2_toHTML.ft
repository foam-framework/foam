              <%= foam.demos.graphics.Dragon.create({y: 200, x: 800, scaleX: 0.6, scaleY: 0.6, timer: foam.util.Timer.create(), blowBubbles: false}) %>
            </p:view>
          </viewChoice>
          <viewChoice>
            <label>Reaction</label>
            <p:view>
              <%
