{
  "name": "QuickBug",
  "version": "1.2",
  "manifest_version": 2,

  "permissions": [
    "https://www.googleapis.com/*",
    "https://code.google.com/p/*",
    "identity",
    "storage",
    "unlimitedStorage"
  ],

  "icons": {
    "64": "images/logo.png"
  },

  "app": {
    "background": {
      "scripts": [
        "foam.js",
        "model/ImportedModels.js",
        "model/Models.js",
        "ui/QIssuePreviewView.js",
        "ui/QIssueCommentView.js",
        "ui/QIssueCommentAuthorView.js",
        "ui/QIssueCommentUpdateView.js",
        "ui/QIssueTileView.js",
        "dao/DAO.js",
        "ui/View.js",
        "model/QBug.js",
        "model/QProject.js",
        "Location.js",
        "Browser.js",
        "ui/PreviewView.js",
        "QueryParser.js",
        "bg.js"
      ]
    }
  },

  "url_handlers": {
    "google_code_issues": {
      "matches": [
        "https://code.google.com/p/*/issues"
      ],
      "title": "Google Code Issues"
    }
  },

   "key": "",

  "oauth2": {
    "client_id": "18229540903-c1cku6fscof2takj43gedatapd7c33m9.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/projecthosting"
    ]
  }
}
